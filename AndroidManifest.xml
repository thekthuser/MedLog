<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.thekthuser.medlog"
      android:versionCode="1"
      android:versionName="1.0">
      <application android:label="@string/app_name" android:theme="@android:style/Theme.Light" android:icon="@drawable/icon">
        <activity android:name="MedLog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.thekthuser.medlog.Profile">
                <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.thekthuser.medlog.main" />
        </activity>
        <activity android:name="com.thekthuser.medlog.Manage" 
        		  android:windowSoftInputMode="adjustResize" >
                <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.thekthuser.medlog.main" />
        </activity>
		<activity android:name="com.thekthuser.medlog.Test">
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thekthuser.medlog.main" />
        </activity>
    </application>
</manifest> 
